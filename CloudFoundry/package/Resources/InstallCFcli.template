FormatVersion: 2.0.0
Version: 1.0.0
Name: Install CF cli

Parameters:
  appName: $appName

Body: |
  return deploy(args.appName).stdout

Scripts:
  deploy:
    Type: Application
    Version: 1.0.0
    EntryPoint: installCFcli.sh
    Files:
      - <common.sh>
    Options:
      captureStdout: true
      captureStderr: true
